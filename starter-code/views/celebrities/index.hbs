{{#each celebrities}}
<h2><a href="/celebrities/{{this._id}}">{{this.name}}</a></h2>
<h2>{{this.occupation}}</h2>
<h2>{{this.catchPhrase}}</h2>
<form action="celebrities/{{this._id}}/delete" method="POST">
 <button type="submit">Delete</button>
</form>
{{/each}}
